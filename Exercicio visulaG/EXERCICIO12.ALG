Algoritmo "exercicio12"

Var
nota1, nota2: real
media : real

Inicio
   escreva("Digite a primeira nota: ")
   leia (nota1)
   escreva("Digite a segunda nota: ")
   leia (nota2)
   
    media <- (nota1 + nota2)/2
    escreva ("A media entre", nota1, " e ", nota2, "e igual a: ",media)
   
   
   

    

Fimalgoritmo